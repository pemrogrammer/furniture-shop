{
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node-dev ./src/index.ts",
        "dev": "tsc -w & nodemon ."
    },
    "dependencies": {
        "express": "^4.17.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    },
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}